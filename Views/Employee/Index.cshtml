@model MVCTraining.Models.Employee

@{
    ViewBag.Title = "Index";
}
<div>
    <h3>Employee Details</h3>
    <hr />
    <div class="row">
        <table>
            <tr>
                <td>Employee ID</td>
                <td>@Model.EmployeeId</td>
            </tr>
            <tr>
                <td>Employee Name</td>
                <td>@Model.Name</td>
            </tr>
            <tr>
                <td>Gender</td>
                <td>@Model.Gender</td>
            </tr>
            <tr>
                <td>Phone Number</td>
                <td>@Model.PhoneNumber</td>
            </tr>
            <tr>
                <td>Email</td>
                <td>@Model.EmailAddress</td>
            </tr>
            <tr>
                <td>Position</td>
                <td>@Model.Position</td>
            </tr>
            <tr>
                <td>Salary</td>
                <td>@Model.Salary</td>
            </tr>
        </table>
    </div>
</div>

